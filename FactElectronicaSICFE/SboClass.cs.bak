            try
            {
                SAPbobsCOM.Recordset oRSMyTable = null;
                oRSMyTable = oCompany.GetBusinessObject(SAPbobsCOM.BoObjectTypes.BoRecordset);
                String query = "SELECT isnull(U_CredCont, 0) as CC FROM [@SIR_PAYT] where Code = '" + pPaymentTerms + "'";

