                            resImpre = clsPrintPDF.PrintPDFs(pArchivo, path.ToString(), versionAcrobat.ToString());
                            if (descEmpresa.ToString().Equals("MEDIT") && idSucursalUsuario < 0 && pArchivo.Contains("Expor")) // Si es la Sucursal Meditian
