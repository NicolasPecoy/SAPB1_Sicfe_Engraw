                            remito.Encabezado = encabezado;

                            // Armo el adenda
                            if (!descEmpresa.ToString().Equals("RURAL"))
                                sicfeAdenda += "Vendedor:        " + obtenerVendedor(oRSMyTable.Fields.Item("SlpCode").Value) + "\r\n"; // Vendedor 
