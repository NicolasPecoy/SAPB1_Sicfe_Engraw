                    catch (Exception exx)
                    {
                        if (guardaLog == true)
                            guardaLogProceso(codigoEnvio.ToString(), codigoFactura, "ERROR al guardar los datos de Respuesta", exx.Message.ToString()); // Guarda log del Proceso
                    }
                }
            }

            try
            {
                if (guardaImagenQR == true)
                {
                    if (String.IsNullOrEmpty(texto))
                    {
