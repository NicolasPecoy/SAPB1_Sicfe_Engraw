                                            decimal pre = precioUniConDesc; // El precio unitario ya viene con el valor que va montoDescuentoLinea
                                            if (iva == 10 || imp.ToString().Equals("IVA_MIN")) // Si es tasa 10 o tiene como codigo IVA_MIN
                                            {
