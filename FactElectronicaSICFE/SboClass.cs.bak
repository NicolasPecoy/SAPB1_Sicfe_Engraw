                    //cnpj = textoReemplazar.Replace("CNPJ", "").Replace("=", "").Replace(":", "").Replace(".", "").Replace("-", "").Replace("/", "").Replace(" ", "");
                }
                catch (Exception ex)
                { }


                return res;
            }
            catch (Exception ex)
            { }
            return res;
        }

